<Window x:Class="PetriPrinter.View.URLdialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PetriPrinter.View"
        mc:Ignorable="d"
        Title="Set DLL url" Height="100" Width="300">
    <DockPanel LastChildFill="False">
        <DockPanel Margin="5" DockPanel.Dock="Top" >
            <TextBlock Margin="0,0,10,0" VerticalAlignment="Center">URL:</TextBlock>
            <TextBox Name="ResponseTextBox"  />
        </DockPanel>
        <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Margin="5" Width="50" IsDefault="True" Click="ButtonOkClick">Ok</Button>
            <Button Margin="5" Width="50" IsCancel="True">Cancel</Button>
        </StackPanel>
    </DockPanel>
</Window>
